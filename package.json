    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^9.0.0",
    "postcss-loader": "^1.2.1",
    "postcss-modules-local-by-default": "^1.1.1",
    "react": "15.4.1",
    "react-addons-test-utils": "15.4.1",
    "react-dom": "15.4.1",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
